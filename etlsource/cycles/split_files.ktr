    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>var max_file_size_env = getEnvironmentVar(&quot;EventLogSplitSize&quot;);
var split_command = null;
var max_file_size = max_file_size_env ? max_file_size_env : 500000;
if (max_file_size &gt; 0 &amp;&amp; getFileSize(filename) &gt; 0) {
	split_command = 
		&quot;split -d -l &quot; + max_file_size + 
		&quot; &quot; + filename + &quot;&quot; + 				&#47;&#47; the file to split
		&quot; &quot; + generate_path + &quot;&#47;split_&quot; + short_filename + &quot;.&quot; ; 	&#47;&#47; the prefix of the output file names
}
		
</jsScript_script>
      </jsScript>    </jsScripts>    <fields>      <field>        <name>split_command</name>
        <rename>split_command</rename>
        <type>String</type>
        <length>-1</length>
        <precision>-1</precision>
        <replace>N</replace>
