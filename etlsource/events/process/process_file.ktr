        <jsScript_script>&#47;&#47;Script here
var client_version_fixed = clientVersion != null ? substr(clientVersion,0,31) : null;
var partnerID = isNum(partnerID) ? partnerID : -1;
var referrer_fixed = referrer != null ? substr(referrer,0,255) : &quot;&quot;;
