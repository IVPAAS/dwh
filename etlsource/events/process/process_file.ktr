var current_point_fixed = isNaN(parseInt(currentPoint)) ? 0 : currentPoint;
var duration_fixed = isNum(duration_fixed) ? duration : null;

if ( currentPoint &lt; 0 ) {
&#47;&#47; _current_point_fixed - is in seconds NOT millis
	switch (eventType) {
		case 4: &#47;&#47; 25%
                _current_point_fixed = duration_fixed&#47;4;
				break;
        case 5: &#47;&#47; 50%
                _current_point_fixed = duration_fixed&#47;2;
